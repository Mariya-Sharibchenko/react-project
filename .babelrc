{
  "presets": ["@babel/preset-env", "@babel/preset-react", "react-app"],
  "plugins": [
    [
    "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": {
          "context": "./src/context",
          "public": "./public",
          "atoms": "./src/atoms",
          "molecules": "./src/molecules"
        }
      }
    ],
    [
      "@babel/plugin-proposal-private-methods",
      {
        "loose": true
      }
    ]
  ]
}